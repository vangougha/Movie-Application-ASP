@model MovieViewModel

<form method="post" asp-action="Add">
    <div class="form-group">
        <label asp-for="MovieName"></label>
        <input asp-for="MovieName" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="MovieDegree"></label>
        <input asp-for="MovieDegree" class="form-control" />
    </div>
    <div>
        <button type="submit" class="btn btn-primary">Add</button>
        <a class="btn btn-warning" asp-controller="Home" asp-action="Index">Home</a>
    </div>
</form>
